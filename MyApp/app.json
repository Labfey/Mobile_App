{
  "expo": {
    "name": "MyApp",
    "slug": "MyApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.labfey.MyApp",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "JeepRoute needs your location to show you on the map.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "JeepRoute needs background location to track your jeep for passengers even when your screen is off.",
        "NSLocationAlwaysUsageDescription": "JeepRoute needs background location to track your jeep for passengers even when your screen is off.",
        "UIBackgroundModes": ["location", "fetch"]
      }
    },
    "android": {
      "package": "com.labfey.MyApp",
      "permissions": [
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_BACKGROUND_LOCATION",
        "android.permission.INTERNET",
        "android.permission.FOREGROUND_SERVICE",
        "android.permission.FOREGROUND_SERVICE_LOCATION",
        "android.permission.ACCESS_NETWORK_STATE",
        "android.permission.WAKE_LOCK"
      ],
      "usesCleartextTraffic": true,
      "googleServicesFile": "./google-services.json"
    },
    "plugins": [
      "expo-router",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow MyApp to use your location even when the app is in the background for mapping features.",
          "locationWhenInUsePermission": "Allow MyApp to use your location to show your position on the map.",
          "isAndroidBackgroundLocationEnabled": true,
          "isAndroidForegroundServiceEnabled": true,
          "foregroundService": {
            "notificationTitle": "JeepRoute Tracking",
            "notificationBody": "Location sharing is active.",
            "notificationColor": "#667EEA"
          }
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "b27eab20-643c-4e06-bcc9-2fc4059a268d"
      }
    }
  }
}